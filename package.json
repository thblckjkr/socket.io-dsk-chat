{
  "name": "socket.io-dsk-chat",
  "version": "0.0.1",
  "description": "A client for socket.io-mdl-chat",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . socket.io-dsk-chat --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Socket.io Desktop chat\"",
    "package-linux": "electron-packager . socket.io-dsk-chat --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": {
    "name": "Teo Gonzalez Calzada",
    "email": "teo.gonzalez.c@hotmail.com"
  },
  "devDependencies": {
    "electron": "^2.0.6",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "muicss": "^0.9.39",
    "socket.io-client": "^2.1.1"
  },
  "license": "Apache-2.0"
}
